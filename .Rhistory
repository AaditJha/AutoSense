#Custom mode function
mymode <- function(col) {
#removing NA values
col <- col[!is.na(col)]
#selecting unique values
uniq <- unique(col)
#finding mode
uniq[which.max(tabulate(match(col, uniq)))]
}
printflm <- function(mdl){
print(summary(mdl)$coefficients)
print(paste("     N:",nobs(mdl),"     ||      R squared:",summary(mdl)$r.squared))
}
